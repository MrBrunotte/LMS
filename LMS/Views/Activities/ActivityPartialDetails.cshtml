@model LMS.Models.Aktivitet
   
<div class="courseInfoBox borderTopBox">
    <div class="row">
        <div class="col-sm-6">
            <h4 class="boxTitle"><span class="spanHeader">Activity: <br /> </span> @Model.Name</h4>
            <h4 class="activityDescription">Activity Description</h4>
            <p class="courseDescriptionBox">@Model.Description</p>
            <H6 class="font-weight-bold">Activity Start: &nbsp;@Model.StartTime.ToString("yyyy-MM-dd | HH:mm")</H6>
            <H6 class="font-weight-bold">Activity End: &nbsp;&nbsp;&nbsp;@Model.EndTime.ToString("yyyy-MM-dd | HH:mm")</H6>
        </div>
        <div class="col-sm-6">
            <div id="activityDocId" class="col-sm-12 activityBox activityDocuments">
            </div>
        </div>
    </div>
</div>