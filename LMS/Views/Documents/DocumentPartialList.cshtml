@model LMS.Models.ViewModels.DocumentListViewModel

<div class="docListHeader">
    @Model.TypeHeader
</div>
<div class="docListBody">
    <div class="docEmptyListMessage">@Model.EmptyMessage</div>
    @foreach (var doc in Model.Documents)
    {
        <ul class="list-group myListGroup">
    <li class="list-group-item myListGroupItem">@doc.Name</li>
        </ul>
    }
</div>
